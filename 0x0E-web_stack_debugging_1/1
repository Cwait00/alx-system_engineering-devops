server {
    listen 80 default_server;
    listen [::]:80 default_server;

    root /var/www/html;
    index index.html index.htm index.nginx-debian.html;

    server_name _;

    location / {
        try_files  / =404;
    }
}
Usage: nginx {start|stop|restart|reload|force-reload|status|configtest|rotate|upgrade}
